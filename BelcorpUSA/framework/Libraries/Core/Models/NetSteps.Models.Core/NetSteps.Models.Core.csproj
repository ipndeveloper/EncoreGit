  <Target Name="AfterBuild" Condition="'$(Configuration)'=='Release'">
    <!-- Get the version number of the main FV assembly to insert into the nuspec files -->
    <MSBuild.ExtensionPack.Framework.Assembly TaskAction="GetInfo" NetAssembly="$(MSBuildProjectDirectory)\bin\$(Configuration)\$(ProjectName).dll">
      <Output TaskParameter="OutputItems" ItemName="Info" />
    </MSBuild.ExtensionPack.Framework.Assembly>
    <Message Text="AssemblyVersion: %(Info.AssemblyVersion)" />
    <Message Text="FileVersion: %(Info.FileVersion)" />
    <!-- Create nuspec file -->
    <Exec Condition="'$(CreateNugetPackage)' == 'true'" Command="..\..\build-tools\nuget spec -F" WorkingDirectory="$(MSBuildProjectDirectory)" />
    <!-- Insert the version number into the nuspec files -->
    <MSBuild.ExtensionPack.Xml.XmlFile Condition="'$(CreateNugetPackage)' == 'true'" TaskAction="UpdateElement" File="$(MSBuildProjectDirectory)\$(ProjectName).nuspec" XPath="/package/metadata/version" InnerText="%(Info.AssemblyVersion)" />
    <!-- Insert Title -->
    <MSBuild.ExtensionPack.Xml.XmlFile Condition="'$(CreateNugetPackage)' == 'true'" TaskAction="UpdateElement" File="$(MSBuildProjectDirectory)\$(ProjectName).nuspec" XPath="/package/metadata/title" InnerText="$(ProjectName)" />
    <!-- Remove License Url -->
    <MSBuild.ExtensionPack.Xml.XmlFile Condition="'$(CreateNugetPackage)' == 'true'" TaskAction="RemoveElement" File="$(MSBuildProjectDirectory)\$(ProjectName).nuspec" XPath="/package/metadata/licenseUrl" />
    <!-- Insert Project Url -->
