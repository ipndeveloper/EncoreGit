    <!-- Remove Icon Url -->
    <MSBuild.ExtensionPack.Xml.XmlFile Condition="'$(CreateNugetPackage)' == 'true'" TaskAction="RemoveElement" File="$(MSBuildProjectDirectory)\$(ProjectName).nuspec" XPath="/package/metadata/iconUrl" />
    <!-- Insert Release Notes -->
    <MSBuild.ExtensionPack.Xml.XmlFile Condition="'$(CreateNugetPackage)' == 'true'" TaskAction="UpdateElement" File="$(MSBuildProjectDirectory)\$(ProjectName).nuspec" XPath="/package/metadata/releaseNotes" InnerText="http://intranet/NetSteps%20Wiki/Encore%20Core%20Release%20Notes.aspx" />
    <!-- Remove Tags -->
    <MSBuild.ExtensionPack.Xml.XmlFile Condition="'$(CreateNugetPackage)' == 'true'" TaskAction="RemoveElement" File="$(MSBuildProjectDirectory)\$(ProjectName).nuspec" XPath="/package/metadata/tags" />
    <!-- Execute the nuspec file to generate package -->
    <Exec Condition="'$(CreateNugetPackage)' == 'true'" Command="..\..\build-tools\nuget pack -Properties Configuration=$(Configuration)" WorkingDirectory="$(MSBuildProjectDirectory)" />
    <!-- Publish the package to both nuget servers-->
