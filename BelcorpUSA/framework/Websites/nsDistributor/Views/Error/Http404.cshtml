@model dynamic
@{
    ViewBag.Title = Html.Term("404Title", "404 - Not Found");
}
<div id="Error404">
    <h2>
        @ViewBag.Title
    </h2>
    <p>
        @Html.Raw(Html.Term("404Text", "We're sorry, but we couldn't find the resource you requested. Would you like to return to the {0}home page{1}?", string.Format("<a href=\"{0}\">", @Url.Content("~/")), "</a>"))
    </p>
</div>

